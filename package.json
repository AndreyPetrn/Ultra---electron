    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "allure-commandline": "^2.24.0",
    "allure-playwright": "^2.8.0",
    "axios": "^1.5.1",
    "cross-fetch": "^4.0.0",
    "dotenv": "^16.3.1",
    "electron": "^25.8.0",
    "electron-playwright-helpers": "^1.2.2",
    "eosjs": "^22.1.0",
    "eslint": "^8.26.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "imap-simple": "^5.1.0",
    "playwright": "^1.39.0",
