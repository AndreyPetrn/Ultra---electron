{
  "author": "Ultra",
  "name": "e2e-tests",
  "version": "0.0.1",
  "description": "e2e-client-electron-playwright",
  "scripts": {
    "eslint:validate": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --color --fix --quiet",
    "test": "playwright test",
    "test:wallet": "npx playwright test --grep @wallet",
    "test:auth": "npx playwright test --grep @authentication",
